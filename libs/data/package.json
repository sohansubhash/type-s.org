{
  "name": "@type-s/data",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate:seed": "NODE_OPTIONS='--import tsx' node generate-seed-sql.ts",
    "seed:local": "NODE_OPTIONS='--import tsx' node generate-seed-sql.ts && wrangler d1 execute type-s-db --local --file ./seed.sql",
    "seed:prod": "NODE_OPTIONS='--import tsx' node generate-seed-sql.ts && wrangler d1 execute type-s-db --remote --file ./seed.sql"
  },
  "exports": {
    "./manufacturers/*": "./manufacturers/*.json",
    "./pcbs/*": "./pcbs/*.json",
    "./controllers/*": "./controllers/*.json",
    "./plates/*": "./plates/*.json",
    "./domes/*": "./domes/*.json",
    "./sliders/*": "./sliders/*.json",
    "./springs/*": "./springs/*.json",
    "./rings/*": "./rings/*.json",
    "./housings/*": "./housings/*.json",
    "./sb-stabs/*": "./sb-stabs/*.json",
    "./wires/*": "./wires/*.json",
    "./cases/*": "./cases/*.json",
    "./models/*": "./models/*.json"
  },
  "dependencies": {
    "@type-s/schema": "workspace:*",
    "drizzle-orm": "^0.44.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250921.0",
    "tsx": "^4.19.2",
    "wrangler": "^4.38.0"
  }
}
