{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "wrangler pages dev ./dist",
    "astro": "astro",
    "db:generate": "drizzle-kit generate",
    "db:init:local": "wrangler d1 execute type-s-db --local --command 'SELECT 1'",
    "db:migrate:local": "wrangler d1 migrations apply type-s-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply type-s-db --remote",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.8",
    "@type-s/data": "workspace:*",
    "@type-s/schema": "workspace:*",
    "astro": "^5.6.1",
    "drizzle-orm": "^0.44.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250921.0",
    "@types/node": "^24.5.2",
    "drizzle-kit": "0.31.7",
    "wrangler": "^4.38.0"
  }
}
